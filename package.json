  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "generatePackage": "cd ./dist/ngx-ui5-web-component-scoping-demo/ && rm -fv ngx-ui5-web-component-scoping-demo-*.tgz  && npm pack",
